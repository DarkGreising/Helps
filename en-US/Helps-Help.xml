<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
<command:details>
<command:name>Convert-Helps</command:name>
<maml:description>
<maml:para>Converts help source scripts into the PowerShell help file.</maml:para>
</maml:description>
<command:verb>Convert</command:verb>
<command:noun>Helps</command:noun>
</command:details>
<maml:description>
<maml:para>Converts help source scripts into the PowerShell help file.</maml:para>
</maml:description>
<command:syntax>
<command:syntaxItem>
<maml:name>Convert-Helps</maml:name>
<command:parameter required="true" position="1" >
<maml:name>Script</maml:name>
<command:parameterValue required="true">String[]</command:parameterValue>
</command:parameter>
<command:parameter required="true" position="2" >
<maml:name>Output</maml:name>
<command:parameterValue required="true">String</command:parameterValue>
</command:parameter>
<command:parameter required="false" position="3" >
<maml:name>Parameters</maml:name>
<command:parameterValue required="true">Hashtable</command:parameterValue>
</command:parameter>
</command:syntaxItem>
</command:syntax>
<command:parameters>
<command:parameter required="true" position="1" >
<maml:name>Script</maml:name>
<maml:description>
<maml:para>Input help source script literal path(s).</maml:para>
</maml:description>
</command:parameter>
<command:parameter required="true" position="2" >
<maml:name>Output</maml:name>
<maml:description>
<maml:para>Output MAML help file (typically &quot;ModuleName.dll-Help.xml&quot;).</maml:para>
</maml:description>
</command:parameter>
<command:parameter required="false" position="3" >
<maml:name>Parameters</maml:name>
<maml:description>
<maml:para>Parameters to be passed in the help source scripts.</maml:para>
</maml:description>
</command:parameter>
</command:parameters>
<command:inputTypes>
<command:inputType>
<dev:type>
<maml:name>None</maml:name>
</dev:type>
</command:inputType>
</command:inputTypes>
<command:returnValues>
<command:returnValue>
<dev:type>
<maml:name>None</maml:name>
</dev:type>
</command:returnValue>
</command:returnValues>
<command:examples>
<command:example>
<maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
<dev:code>Import-Module Helps
Convert-Helps Helps-Help.ps1 temp.xml</dev:code>
<dev:remarks>
<maml:para>It builds this module help file &quot;temp.xml&quot; from the help script &quot;Helps-Help.ps1&quot;.</maml:para>
</dev:remarks>
</command:example>
</command:examples>
